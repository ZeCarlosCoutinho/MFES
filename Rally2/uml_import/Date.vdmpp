class Date
instance variables
  year:int;
  month:int;
  day:int;
  hours:int;
  minutes:int;
  seconds:int;

operations
  public  Date : int * int * int * int * int * int ==> Date
  Date(yr, mnth, d, hrs, mnts, scnds) ==
  (
  	year := yr;
  	month := mnth;
  	day := d;
  	hours := hrs;
  	minutes := mnts;
  	seconds := scnds;
  );

end Date
