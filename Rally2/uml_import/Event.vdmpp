class Event
instance variables
  public teams:set of (Team);
  public beginning_date:Date;
  public end_date:Date;
  public name:seq of (char);
  public location:seq of (char);

operations
  public  addTeam : Team ==> ()
  addTeam(team) ==
    (
    teams := teams union {team}
    )
    pre team not in set teams
    post team in set teams;

end Event
